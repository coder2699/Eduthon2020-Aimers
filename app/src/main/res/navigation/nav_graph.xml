<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/splashFragment">

    <fragment
        android:id="@+id/splashFragment"
        android:name="com.example.eduthon_aimers.SplashFragment"
        android:label="fragment_splash"
        tools:layout="@layout/fragment_splash" >
        <action
            android:id="@+id/action_splashFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            app:popUpTo="@id/splashFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.eduthon_aimers.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_faqsFragment"
            app:destination="@id/faqsFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"
            app:popUpTo="@id/homeFragment" />
        <action
            android:id="@+id/action_homeFragment_to_articleFragment"
            app:destination="@id/articleFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"
            app:popUpTo="@id/homeFragment" />
        <action
            android:id="@+id/action_homeFragment_to_referenceFragment"
            app:destination="@id/referenceFragment" />
        <action
            android:id="@+id/action_homeFragment_to_healthFragment"
            app:destination="@id/healthFragment" />
        <action
            android:id="@+id/action_homeFragment_to_cyberFragment"
            app:destination="@id/cyberFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"
            app:popUpTo="@id/homeFragment" />
        <action
            android:id="@+id/action_homeFragment_to_donateFragment"
            app:destination="@id/donateFragment" />
        <action
            android:id="@+id/action_homeFragment_to_videoActivity"
            app:destination="@id/videoActivity"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"
            app:popUpTo="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/articleFragment"
        android:name="com.example.eduthon_aimers.Articles.ArticleFragment"
        android:label="fragment_article"
        tools:layout="@layout/fragment_article" >
        <action
            android:id="@+id/action_articleFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_articleFragment_to_donateFragment"
            app:destination="@id/donateFragment" />
    </fragment>
    <fragment
        android:id="@+id/faqsFragment"
        android:name="com.example.eduthon_aimers.Faq.FaqsFragment"
        android:label="fragment_faqs"
        tools:layout="@layout/fragment_faqs" >
        <action
            android:id="@+id/action_faqsFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_faqsFragment_to_donateFragment"
            app:destination="@id/donateFragment" />
    </fragment>
    <fragment
        android:id="@+id/healthFragment"
        android:name="com.example.eduthon_aimers.HealthFragment"
        android:label="fragment_health"
        tools:layout="@layout/fragment_health" >
        <action
            android:id="@+id/action_healthFragment_to_yogaFragment"
            app:destination="@id/yogaFragment" />
        <action
            android:id="@+id/action_healthFragment_to_backFragment"
            app:destination="@id/backFragment" />
        <action
            android:id="@+id/action_healthFragment_to_eyesFragment"
            app:destination="@id/eyesFragment" />
        <action
            android:id="@+id/action_healthFragment_to_waterFragment"
            app:destination="@id/waterFragment" />
        <action
            android:id="@+id/action_healthFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_healthFragment_to_donateFragment"
            app:destination="@id/donateFragment" />
    </fragment>
    <fragment
        android:id="@+id/referenceFragment"
        android:name="com.example.eduthon_aimers.ReferenceFragment"
        android:label="fragment_reference"
        tools:layout="@layout/fragment_reference" >
        <action
            android:id="@+id/action_referenceFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_referenceFragment_to_donateFragment"
            app:destination="@id/donateFragment" />
    </fragment>
    <fragment
        android:id="@+id/cyberFragment"
        android:name="com.example.eduthon_aimers.Cyber.CyberFragment"
        android:label="fragment_cyber"
        tools:layout="@layout/fragment_cyber" >
        <action
            android:id="@+id/action_cyberFragment_to_solutionFragment"
            app:destination="@id/solutionFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"
            app:popUpTo="@id/cyberFragment" />
        <action
            android:id="@+id/action_cyberFragment_to_complaintFragment"
            app:destination="@id/complaintFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"
            app:popUpTo="@id/cyberFragment" />
        <action
            android:id="@+id/action_cyberFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_cyberFragment_to_donateFragment"
            app:destination="@id/donateFragment" />
    </fragment>
    <fragment
        android:id="@+id/complaintFragment"
        android:name="com.example.eduthon_aimers.Cyber.ComplaintFragment"
        android:label="fragment_complaint"
        tools:layout="@layout/fragment_complaint" >
        <action
            android:id="@+id/action_complaintFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_complaintFragment_to_donateFragment"
            app:destination="@id/donateFragment" />
    </fragment>
    <fragment
        android:id="@+id/solutionFragment"
        android:name="com.example.eduthon_aimers.Cyber.SolutionFragment"
        android:label="fragment_solution"
        tools:layout="@layout/fragment_solution" >
        <action
            android:id="@+id/action_solutionFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_solutionFragment_to_donateFragment"
            app:destination="@id/donateFragment" />
    </fragment>
    <fragment
        android:id="@+id/backFragment"
        android:name="com.example.eduthon_aimers.Health.BackFragment"
        android:label="fragment_back"
        tools:layout="@layout/fragment_back" >
        <action
            android:id="@+id/action_backFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_backFragment_to_donateFragment"
            app:destination="@id/donateFragment" />
    </fragment>
    <fragment
        android:id="@+id/eyesFragment"
        android:name="com.example.eduthon_aimers.Health.EyesFragment"
        android:label="fragment_eyes"
        tools:layout="@layout/fragment_eyes" >
        <action
            android:id="@+id/action_eyesFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_eyesFragment_to_donateFragment"
            app:destination="@id/donateFragment" />
    </fragment>
    <fragment
        android:id="@+id/waterFragment"
        android:name="com.example.eduthon_aimers.Health.WaterFragment"
        android:label="fragment_water"
        tools:layout="@layout/fragment_water" >
        <action
            android:id="@+id/action_waterFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_waterFragment_to_donateFragment"
            app:destination="@id/donateFragment" />
    </fragment>
    <fragment
        android:id="@+id/yogaFragment"
        android:name="com.example.eduthon_aimers.Health.YogaFragment"
        android:label="fragment_yoga"
        tools:layout="@layout/fragment_yoga" >
        <action
            android:id="@+id/action_yogaFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_yogaFragment_to_donateFragment"
            app:destination="@id/donateFragment" />
    </fragment>
    <fragment
        android:id="@+id/donateFragment"
        android:name="com.example.eduthon_aimers.Donation.DonateFragment"
        android:label="DonateFragment" />
    <activity
        android:id="@+id/videoActivity"
        android:name="com.example.eduthon_aimers.Video.VideoActivity"
        android:label="VideoActivity" />
</navigation>